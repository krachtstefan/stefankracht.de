{
  "version": 2,
  "alias": "stefankracht.now.sh",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    { "src": "/page/(?<page>[^/]+)$", "dest": "/blog?page=$page" },
    {
      "src": "/blog/(?<category>[^/]+)$",
      "dest": "/blog-category?category=$category"
    },
    { "src": "/p/(?<url>[^/]+)$", "dest": "/post?url=$url" },
    {
      "src": "/contact",
      "status": 301,
      "headers": { "Location": "/about" }
    },
    {
      "src": "/about-me|/about-me/",
      "status": 301,
      "headers": { "Location": "/about" }
    },
    {
      "src": "/site-search|/site-search/",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/news/(?<url>[^/]+)$",
      "status": 301,
      "headers": { "Location": "/p/$url" }
    }
  ]
}
